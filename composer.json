{
  "name": "tourze/train-institution-bundle",
  "description": "培训机构管理",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/doctrine-bridge": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/routing": "^6.4",
    "symfony/uid": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TrainInstitutionBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TrainInstitutionBundle\\Tests\\": "tests"
    }
  }
}